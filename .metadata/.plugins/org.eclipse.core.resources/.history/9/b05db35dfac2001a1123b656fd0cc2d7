package test;

public class Test {

	public static void main(String[] args) {
		String s = "momomo";
		for(int i = 0; i < s.length(); i++) {
    		for(int j = i; j < s.length(); j++) {
    			System.out.println(s.substring(i, j));
    			//substrings.put(s.substring(i, j));
    		}
    		
    	}
	}

}


